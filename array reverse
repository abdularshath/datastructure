/******************************************************************************

array insert element at any position by specifying the position index:

*******************************************************************************/
#include <stdio.h>

int main()
{
    int n,ind,val;
    scanf("%d",&n);
    int a[n],temp[n];
    
    
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    for(int i=0;i<n;i++){
        printf("%d",a[i]);
    }
    for(int i=0;i<n;i++){
        temp[i]=a[n-i-1];
        
    }
    

    printf("\n");
    for(int i=0;i<n;i++){
        printf("%d",temp[i]);
    }
    
    return 0;
}
